    <padding>
        <Insets bottom="40.0" left="40.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" alignment="BOTTOM_RIGHT" />
    <Button onAction="#onHelloButtonClick" text="Hello!" />
   <ToolBar prefHeight="17.0" prefWidth="626.0">
     <items>
       <Button mnemonicParsing="false" text="Button" />
     </items>
   </ToolBar>
</VBox>
